class Solution:
    def leftRightDifference(self, nums: List[int]) -> List[int]:
        leftSum = [0,]
        rightSum = [0,]
        nums2=nums[::-1]
        for i in range(1,len(nums)):
            leftSum.append(leftSum[i-1]+nums[i-1])
            rightSum.append(rightSum[i-1]+nums2[i-1])
        return [abs(x-y) for x,y in zip(leftSum,rightSum[::-1])]
        
