class Solution:
    def commonChars(self, words: List[str]) -> List[str]:
        common=Counter(words[0])
        for i in range(1,len(words)):
            dic=Counter(words[i])
            for j in dict(common):
                if j in dic:
                    common[j]=min([dic[j],common[j]])
                else:
                    common.pop(j)
        res=[]
        for i in common:
            for j in range(common[i]):
                res.append(i)
        return sorted(res)
        
