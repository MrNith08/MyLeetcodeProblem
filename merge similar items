class Solution:
    def mergeSimilarItems(self, items1: List[List[int]], items2: List[List[int]]) -> List[List[int]]:
        dic1={x:y for x,y in items1}
        dic2={x:y for x,y in items2}
        for i,j in dic2.items():
            if i in dic1:
                dic1[i]+=j
            else:
                dic1[i]=j
        res=[]
        for x,y in dic1.items():
            res.append([x,y])
        return sorted(res)
