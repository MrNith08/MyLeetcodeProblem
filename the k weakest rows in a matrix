class Solution:
    def kWeakestRows(self, mat: List[List[int]], k: int) -> List[int]:
        lst=[]
        for i in mat:
            lst.append(i.count(1))
        res=[]
        for i in range(k):
            ind=lst.index(min(lst))
            res.append(ind)
            lst[ind]=1000
        return res
