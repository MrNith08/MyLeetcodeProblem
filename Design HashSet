class MyHashSet:

    def __init__(self):
        self.x=[]  
    def add(self, key: int) -> None:
        if key not in self.x:
            self.x+= [key]
    def remove(self, key: int) -> None:
        for i in self.x:
            if key==i:
                self.x.remove(i)
    def contains(self, key: int) -> bool:
        for i in self.x:
            if key == i:
                return True
        return False
        
# Your MyHashSet object will be instantiated and called as such:
# obj = MyHashSet()
# obj.add(key)
# obj.remove(key)
# param_3 = obj.contains(key)
