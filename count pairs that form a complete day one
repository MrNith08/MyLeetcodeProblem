class Solution:
    def countCompleteDayPairs(self, hours: List[int]) -> int:
        remainders = [h % 24 for h in hours]
        dic = Counter(remainders)
        pairs = 0
        pairs += dic[0] * (dic[0] - 1) // 2
        pairs += dic[12] * (dic[12] - 1) // 2
        for r in range(1, 12):  
            pairs += dic[r] * dic[24 - r]
        return pairs
