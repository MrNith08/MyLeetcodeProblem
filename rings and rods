class Solution:
    def countPoints(self, rings: str) -> int:
        dic={i:set() for i in range(10)}
        for i in range(0,len(rings),2):
            dic[int(rings[i+1])].add(rings[i])
        count=0
        for i in dic:
            if len(dic[i])==3:
                count+=1
        return count
