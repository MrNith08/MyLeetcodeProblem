class Solution:
    def findMissingAndRepeatedValues(self, grid: List[List[int]]) -> List[int]:
        lst=[x for y in grid for x in y]
        dic=Counter(lst)
        keys=dic.keys()
        r,s=0,0
        for i in range(1,len(grid)**2+1):
            if dic[i]==2:
                r=i
            if dic[i]==0:
                s=i
        return[r,s]
