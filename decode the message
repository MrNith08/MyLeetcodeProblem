class Solution:
    def decodeMessage(self, key: str, message: str) -> str:
        decode={}
        keys=[]
        for i in key:
            if i!=" " and i not in keys:
                keys.append(i)
        for i in range(0,26):
            decode[keys[i]]=chr(i+97)
        res=""
        for i in message:
            if i==" ":
                res=res+" "
            else:
                res=res+decode[i]
        return res
        
