class Solution:
    def mergeArrays(self, nums1: List[List[int]], nums2: List[List[int]]) -> List[List[int]]:
        nums1.extend(nums2)
        nums1.sort()
        res=[]
        i=0
        while i <len(nums1):
            if i<len(nums1)-1 and nums1[i][0]==nums1[i+1][0]:
                res.append([nums1[i][0],nums1[i][1]+nums1[i+1][1]])
                i+=1
            else:
                res.append(nums1[i])
            i+=1
        return res
